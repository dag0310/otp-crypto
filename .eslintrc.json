{
  "extends": [
    "standard",
    "plugin:jasmine/recommended"
  ],
  "plugins": [
    "jasmine"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es2022": true,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"]
  }
}
